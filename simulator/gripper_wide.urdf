<?xml version="1.0"?>
<robot name="gripper">

    <material name="red">
        <color rgba="0.8 0 0 1"/>
    </material>
    <material name="green">
        <color rgba="0 0.8 0 1"/>
    </material>
    <material name="yellow">
        <color rgba="0.8 0.8 0 1"/>
    </material>

    <link name="world">
        <inertial>
            <mass value="0"/>
            <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1"/>
        </inertial>
    </link>
    <link name="x">
        <inertial>
            <mass value="0"/>
            <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1"/>
        </inertial>
    </link>
    <joint name="x_joint" type="prismatic">
        <parent link="world"/>
        <child link="x"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <limit effort="1000.0" lower="-10" upper="10" velocity="0.01"/>
        <axis xyz="1 0 0"/>
    </joint>
    <link name="y">
        <inertial>
            <mass value="0"/>
            <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1"/>
        </inertial>
    </link>
    <joint name="y_joint" type="prismatic">
        <parent link="x"/>
        <child link="y"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <limit effort="1000.0" lower="-10" upper="10" velocity="0.01"/>
        <axis xyz="0 1 0"/>
    </joint>
    <link name="z">
        <inertial>
            <mass value="0"/>
            <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1"/>
        </inertial>
    </link>
    <joint name="z_joint" type="prismatic">
        <parent link="y"/>
        <child link="z"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <limit effort="1000.0" lower="-10" upper="10" velocity="0.01"/>
        <axis xyz="0 0 1"/>
    </joint>
    <link name="r">
        <inertial>
            <mass value="0"/>
            <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1"/>
        </inertial>
    </link>
    <joint name="r_joint" type="revolute">
        <parent link="z"/>
        <child link="r"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <limit effort="1000.0" lower="-1.58" upper="1.58" velocity="0.01"/>
        <axis xyz="1 0 0"/>
    </joint>
    <link name="p">
        <inertial>
            <mass value="0"/>
            <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1"/>
        </inertial>
    </link>
    <joint name="p_joint" type="revolute">
        <parent link="r"/>
        <child link="p"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <limit effort="1000.0" lower="-1.58" upper="1.58" velocity="0.01"/>
        <axis xyz="0 1 0"/>
    </joint>


    <link name="base_link">
        <visual>
            <geometry>
                <box size="0.32 0.01 0.06"/>
            </geometry>
            <material name="yellow"/>
        </visual>
        <collision>
            <geometry>
                <box size="0.32 0.01 0.06"/>
            </geometry>
            <material name="yellow"/>
        </collision>
        <inertial>
            <mass value="1"/>
            <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1"/>
        </inertial>
    </link>
    <joint name="yy_joint" type="revolute">
        <parent link="p"/>
        <child link="base_link"/>
        <origin xyz="0 0 0" rpy="-1.57 0 0"/>
        <limit effort="1000.0" lower="-1.58" upper="1.58" velocity="0.5"/>
        <axis xyz="0 -1 0"/>
    </joint>

    <link name="left_jaw">
        <visual>
            <geometry>
                <box size="0.05 0.01 0.06"/>
            </geometry>
            <origin rpy="0 0 1.57" xyz="-0.155 0.03 0"/>
            <material name="red"/>
        </visual>
        <collision>
            <geometry>
                <box size="0.05 0.01 0.06"/>
            </geometry>
            <origin rpy="0 0 1.57" xyz="-0.155 0.03 0"/>
        </collision>
        <contact>
            <lateral_friction value="1"/>
        </contact>
        <inertial>
            <mass value="1"/>
            <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1"/>
        </inertial>
    </link>
    <joint name="left_joint" type="prismatic">
        <parent link="base_link"/>
        <child link="left_jaw"/>
        <limit effort="1000.0" lower="0" upper="0.15"/>
        <axis xyz="1 0 0"/>
    </joint>

    <link name="right_jaw">
        <visual>
            <geometry>
                <box size="0.05 0.01 0.06"/>
            </geometry>
            <origin rpy="0 0 1.57" xyz="0.155 0.03 0"/>
            <material name="green"/>
        </visual>
        <collision>
            <geometry>
                <box size="0.05 0.01 0.06"/>
            </geometry>
            <origin rpy="0 0 1.57" xyz="0.155 0.03 0"/>
        </collision>
        <contact>
            <lateral_friction value="1"/>
        </contact>
        <inertial>
            <mass value="1"/>
            <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1"/>
        </inertial>
    </link>
    <joint name="right_joint" type="prismatic">
        <parent link="base_link"/>
        <child link="right_jaw"/>
        <limit effort="1000.0" lower="0" upper="0.15"/>
        <axis xyz="-1 0 0"/>
    </joint>
</robot>
